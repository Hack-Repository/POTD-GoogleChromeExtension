{
  "manifest_version": 3,
  "name": "POTD - Problem Of The Day",
  "description": "Solve a new DSA problem every day.",
  "version": "1.0.0.1",
  "icons": {
    "16": "/assets/POTD_logo_16.png",
    "48": "/assets/POTD_logo_48.png",
    "128": "/assets/POTD_logo_128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icons": {
      "16": "/assets/POTD_logo_16.png",
      "48": "/assets/POTD_logo_48.png",
      "128": "/assets/POTD_logo_128.png"
    }
  },
  "content_scripts": [
    {
      "js": [
        "./popup.js"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "storage"
  ]
}